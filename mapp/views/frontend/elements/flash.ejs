
<% Object.keys(messages).forEach((type) => { %>    
    <div class="alert alert-<%= (type) ==='error'? 'danger' : type %>" role="alert">     
       <% messages[type].forEach((message) => { %>
             <span class="close-btn">&times;</span> <%= message %>
        <% }) %>
    </div>
<% }) %>





